{
  "name": "docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "prerender": "ts-node build/prerender",
    "build": "vue-cli-service build && yarn prerender",
    "test:unit": "vue-cli-service test:unit",
