{
  "name": "buetify",
  "version": "0.1.1",
  "description": "A Vue component framework based on Bulma",
  "main": "./src/index.ts",
  "author": {
    "name": "Harry Kightlinger",
    "email": "kightlingerh@gmail.com"
  },
  "scripts": {
    "test": "echo \"No test specified\"",
    "publish": "yarn run build && yarn run np",
    "np": "np",
    "build": "yarn run prettier-write && node build/build-lib.js",
    "prettier-write": "prettier --write \"./src/**/*.ts\"",
    "prettier-check": "prettier --list-different \"./src/**/*.ts\"",
    "tsc": "tsc"
  },
  "license": "MIT",
  "sideEffects": [
    "*.css",
    "*.sass",
    "*.scss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kightlingerh/buetify"
  },
  "bugs": {
    "url": "https://github.com/kightlingerh/buetify/issues"
  },
  "files": [
    "src/",
    "es5/",
    "lib/"
  ],
  "keywords": [
    "bulma",
    "ui framework",
    "vue framework",
    "ui library",
    "component framework"
  ],
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "dependencies": {
    "core-js": "^3.4.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/vue-fontawesome": "^0.1.8",
    "bulma": "^0.8.0",
    "fp-ts": "^2.2.0",
    "fp-ts-local-storage": "^0.2.0",
    "lodash.debounce": "^4.0.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@types/lodash.debounce": "^4.0.6",
    "babel-plugin-detective": "^2.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-define": "^2.0.0",
    "cross-env": "^7.0.0",
    "np": "^5.2.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1",
    "shelljs": "^0.8.3",
    "typescript": "^3.7.5",
    "vue": "^2.6.11",
    "vue-meta": "^2.3.1"
  }
}
